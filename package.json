{
  "name": "domain-inspector-plus",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev:chrome": "vite --mode chrome",
    "build:chrome": "vue-tsc --noEmit && vite build --mode chrome",
    "type-check": "vue-tsc --noEmit",

    "format": "prettier --write .",
    "format:check": "prettier --check .",

    "lint": "eslint . --ext .vue,.js,.ts --fix",
    "lint:check": "eslint . --ext .vue,.js,.ts",

    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "clipboard": "^2.0.11",
    "vue": "^3.4.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "postcss": "^8.4.32",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-web-extension": "^4.0.0",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}